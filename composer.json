{
    "name": "flyingbono/smart-bot",
    "description": "Smart multilinguage bot responder implemented in PHP, with learning capabilities",
    "keywords": ["bot", "learn", "chat", "php", "multilinguage"],
    "authors": [
        {
            "name": "Bruno VIBERT",
            "email": "bruno.vibert@bonobox.fr"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "type": "library",
    "require": {
        "php": ">=5.5.13",
        "php-di/php-di": "^5.1"
    },
    "license": "Apache-2.0",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "flyingbono/smart-bot",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/flyingbono/SmartBot.git",
                    "type": "vcs"
                },
                "autoload": {
                    "psr-4" : {
                        "SmartBot\\" : "lib/SmartBot",
                        "SmartBot\\Bot\\" : "lib/SmartBot/Bot"
                    }
                }
            }
        }
    ]
    
}
