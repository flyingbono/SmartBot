{
    "name": "flyingbono/smart-bot",
    "description": "Smart multilinguage bot responder implemented in PHP, with learning capabilities",
    "keywords": ["bot", "learn", "chat", "php", "multilinguage"],
    "authors": [
        {
            "name": "Bruno VIBERT",
            "email": "bruno.vibert@bonobox.fr"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "type": "library",
    "require": {
        "php": ">=5.5.13",
        "php-di/php-di": "^5.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "doctrine/annotations": "~1.2"
    },
    "support": {
        "issues": "https://github.com/flyingbono/SmartBot/issues",
        "wiki": "https://github.com/flyingbono/SmartBot/wiki",
        "source": "https://github.com/flyingbono/SmartBot"
    },
    "license": "Apache-2.0",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "flyingbono/smart-bot",
                "version": "1.0.x-dev",
                "source": {
                    "url": "https://github.com/flyingbono/SmartBot.git",
                    "type": "git",
                    "reference": "dev-master"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "SmartBot":        "lib/SmartBot"
        },
        "psr-4" : {
            "SmartBot\\" : "lib/SmartBot",
            "SmartBot\\Bot\\" : "lib/SmartBot/Bot"
        }
    }
    
}
